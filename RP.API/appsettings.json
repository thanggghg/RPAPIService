{
  "ConnectionStrings": {
    "SQLDB": "Server={PR_DBHOST},{PR_DBPORT};Database={PR_DB};User Id={PR_DBUSERNAME};Password={PR_DBPASSWORD};Encrypt=True;TrustServerCertificate=True;"
  },
  "JwtOptions": {
    "Issuer": "issuer",
    "Audience": "audience",
    "SigningKey": "{SIGNING_KEY}",
    "ExpirationSeconds": 36000,
    "RefreshTokenExpirationSeconds": 72000
  },
  "AllowedOrigins": "{ALLOWED_ORIGINS}"
}